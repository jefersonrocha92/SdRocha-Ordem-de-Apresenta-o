import random

n1 = str(input('Primeiro candidato: '))
n2 = str(input('Segundo candidato: '))
n3 = str(input('Terceiro candidato: '))
n4 = str(input('Quarto candidato: '))
lista = [n1, n2, n3, n4]

random.shuffle(lista)
print('A ordem de apresentação será')
print(lista)


